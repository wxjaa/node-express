<nav class="navbar-side" role="navigation">
    <div class="navbar-collapse sidebar-collapse collapse">
        <ul id="side" class="nav navbar-nav side-nav">
            <% for (var i = 0, l = menu.length; i < l; i++) { %>
            <li class="panel">
                <% if(menu[i].isDisplay == 1){ %>
                <a href="/<%= menu[i].url %>" data-toggle="collapse" class="accordion-toggle"
                   data-target="#<%= menu[i].menuId %>"><i class="fa fa-cogs"></i> <%= menu[i].name %> <span
                            class="fa arrow"></span></a>
                <% if(menu[i].menus.length > 0){ %>
                <ul class="collapse nav" id="components">
                    <% for (var m = 0, n = menu[i].menus.length; m < n; m++) { %>
                    <li>
                        <a href="/<%= menu[i].menus[m].url %>" data-id="<%= menu[i].menus[m].menuId %>"><i
                                    class="fa fa-angle-double-right"></i> <%= menu[i].menus[m].name %></a>
                        <% if(menu[i].menus[m].menus.length > 0){ %>
                        <ul class="collapse nav" id="components">
                            <% for (var x = 0, y = menu[i].menus[m].menus.length; x < y; x++) { %>
                            <li>
                                <a href="/<%= menu[i].menus[m].menus[x].url %>"
                                   data-id="<%= menu[i].menus[m].menus[x].menuId %>"><i
                                            class="fa fa-angle-double-right"></i> <%= menu[i].menus[m].menus[x].name %>
                                </a>
                            </li>
                            <% } %>
                        </ul>
                        <% } %>
                    </li>
                    <% } %>
                </ul>
                <% } %>
                <% } %>
            </li>
            <% } %>
        </ul>
    </div>
</nav>